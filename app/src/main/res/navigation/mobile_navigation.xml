<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_explore"
        android:name="com.ingjuanocampo.sazonlife.ui.home.HomeFragment"
        android:label="@string/title_task"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.ingjuanocampo.sazonlife.ui.explore.ExploreFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_favorite"
        android:name="com.ingjuanocampo.sazonlife.ui.favorite.FavoriteFragment"
        android:label="@string/title_users"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/createFragment"
        android:name="com.ingjuanocampo.sazonlife.ui.create.CreateFragment"
        android:label="create_fragment"
        tools:layout="@layout/create_fragment" />

    <fragment
        android:id="@+id/navigation_shop"
        android:name="com.ingjuanocampo.sazonlife.ui.store.StoreFragment"
        android:label="store_fragment"
        tools:layout="@layout/store_fragment" />
</navigation>